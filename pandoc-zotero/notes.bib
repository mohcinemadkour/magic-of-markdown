  keywords = {Biological modelling,Knowledge integration,reverse engineering,Systems Biology},
  pages = {86--103},
  file = {ScienceDirect Full Text PDF:/Users/roshi/Library/Application Support/Zotero/Profiles/v61csx6s.default/zotero/storage/72W9GKRU/Hecker et al. - 2009 - Gene regulatory network inference Data integratio.pdf:application/pdf;ScienceDirect Snapshot:/Users/roshi/Library/Application Support/Zotero/Profiles/v61csx6s.default/zotero/storage/P76W5FWT/S0303264708002608.html:text/html}
