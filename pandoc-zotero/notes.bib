  pages = {e7686081},
  file = {Snapshot:/Users/roshi/Library/Application Support/Zotero/Profiles/v61csx6s.default/zotero/storage/Q77BQS7K/7686081.html:application/xhtml+xml;Full Text PDF:/Users/roshi/Library/Application Support/Zotero/Profiles/v61csx6s.default/zotero/storage/W253HQQT/Anderson et al. - 2016 - Boolean Modeling of Cellular and Molecular Pathway.pdf:application/pdf}
